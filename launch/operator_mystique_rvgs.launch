<launch>
    <rosparam command="load" file="$(find project11)/parameters/rvgs_operator.yaml" />
    <arg name="host" default="mystiquec"/>
    <node pkg="udp_bridge" type="operator_node" name="udp_bridge_operator" args="$(arg host)"/>
    <node pkg="flir" type="flir_decode_node.py" name="flir_decode_node"/>
    <node pkg="range_and_bearing" type="range_and_bearing_node" name="range_and_bearing">
        <remap from="/input1" to="/base/position"/>
        <remap from="/input2" to="/udp/posmv/position"/>
    </node>
    <node pkg="command_bridge" type="command_bridge_sender_node.py" name="command_bridge_sender"/>
    <node pkg="project11_transformations" type="project11_transformations_node" name="project11_transformations"/>
    <node pkg="joy" type="joy_node" name="joy_node">
        <param name="autorepeat_rate" value="10.0"/>
    </node>
    <node pkg="joy_to_helm" type="joy_to_helm.py" name="joy_to_helm"/>
    <node pkg="posmv" type="posmv_nmea_node.py" name="posmv_nmea"/>
    <node pkg="mbr" type="mbr_node.py" name="mbr_node" />
    <node pkg="mbr" type="mbr_logging.py" name="mbr_logging" />

    <node pkg="rosbag" type="record" name="operator_logging" args="-o $(find project11)/logs/operator
        /base/camera/look_at
        /base/camera/look_at_mode
        /base/position
        /base/time_reference
        /bearing
        /diagnostics
        /joy
        /mbr/2510/9372/dist_age
        /mbr/2510/9372/distance
        /mbr/2510/9372/id
        /mbr/2510/9372/margin_avg
        /mbr/2510/9372/margin_min
        /mbr/2510/9372/pitch
        /mbr/2510/9372/rx_age
        /mbr/2510/9372/yaw
        /mbr/2510/Total_RX_bandwidth_from_mac_kb_per_s
        /mbr/2510/Total_TX_bandwidth_to_mac_kb_per_s
        /odom
        /origin
        /position_map
        /position_utm
        /range
        /rosmon_1557409110443660171/state
        /rosout
        /rosout_agg
        /send_command
        /tf
        /tf_static
        /udp/appcast
        /udp/command
        /udp/contact
        /udp/coverage
        /udp/darknet_ros/bounding_boxes
        /udp/diagnostics
        /udp/flir_engine
        /udp/heading
        /udp/heartbeat
        /udp/helm
        /udp/helm_mode
        /udp/loiter_updates
        /udp/mbes_ping
        /udp/mbr/9372/2510/margin_avg
        /udp/mbr/9372/2510/margin_min
        /udp/mission_plan
        /udp/origin
        /udp/position
        /udp/posmv/orientation
        /udp/posmv/position
        /udp/project11/display
        /udp/radar
        /udp/response
        /udp/sog
        /udp/wpt_updates
    " />
    
</launch>



